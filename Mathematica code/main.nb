      RowBox[{
      "thefront", ",", "theside", ",", "doesFloat", ",", "doesFloatFlat", ",",
        " ", "trimAngle", ",", "stabilityPlot", ",", "stabilityAngles", ",", 
       "bounds", ",", "floatHeight", ",", "mass", ",", "stabilitydata", ",", 
       "interp", ",", "area", ",", "area2", ",", "almostUp"}], "}"}], ",", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"thefront", "=", 
       RowBox[{"front", "/.", "boat"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"theside", "=", 
       RowBox[{
        RowBox[{"RotationMatrix", "[", 
         RowBox[{
          RowBox[{"Pi", "/", "2"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], "]"}], ".", 
        "thefront"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"doesFloat", "=", 
