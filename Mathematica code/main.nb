          TemplateBox[{
           "5",RowBox[{"\[InvisibleSpace]", "\"\[Degree]\""}],"degrees",
            "\"AngularDegrees\""},
           "QuantityPostfix"], ",", "thefront"}], "]"}], ".", 
        RowBox[{"{", 
         RowBox[{"0", ",", "0", ",", "1"}], "}"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Grid", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Style", "[", 
             RowBox[{
              RowBox[{"name", "/.", "boat"}], ",", 
              RowBox[{"FontSize", "\[Rule]", "60"}], ",", 
              RowBox[{"FontFamily", "\[Rule]", "\"\<Purisa\>\""}]}], "]"}], 
            ",", "SpanFromLeft"}], "}"}], ",", "\[IndentingNewLine]", 
