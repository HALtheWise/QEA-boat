           RowBox[{"-", "Pi"}], ",", "Pi"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"bounds", "=", 
       RowBox[{
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"RegionBounds", "[", 
            RowBox[{"region", "/.", "boat"}], "]"}], "/.", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"min_", ",", "max_"}], "}"}], "\[RuleDelayed]", 
            RowBox[{"max", "-", "min"}]}]}], ",", "0.1"}], "]"}], 
        TemplateBox[{
         InterpretationBox["\[InvisibleSpace]", 1],"\"cm\"","centimeters",
          "\"Centimeters\""},
         "Quantity"]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"floatHeight", "=", 
       RowBox[{"boatWaterline", "[", 
        RowBox[{"boat", ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"area", "=", 
       RowBox[{"underwaterArea", "[", 
        RowBox[{"boat", ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0", ",", "1"}], "}"}], ",", "floatHeight"}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"area2", "=", 
       RowBox[{"frontalArea", "[", 
        RowBox[{"boat", ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0", ",", "1"}], "}"}], ",", "floatHeight", ",", 
