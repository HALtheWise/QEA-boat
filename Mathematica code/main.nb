             SuperscriptBox["\"cm\"", "3"]}],"centimeters cubed",
           SuperscriptBox["\"Centimeters\"", "3"]},
          "Quantity"], "*", 
         TemplateBox[{
          "1",RowBox[{
            "\"g\"", "\[InvisibleSpace]", "\"/\"", "\[InvisibleSpace]", 
             SuperscriptBox["\"cm\"", "3"]}],"grams per centimeter cubed",
           FractionBox["\"Grams\"", 
            SuperscriptBox["\"Centimeters\"", "3"]]},
          "Quantity"]}], ">", 
        RowBox[{
         RowBox[{"totalMass", "[", "boat", "]"}], 
         TemplateBox[{
          InterpretationBox["\[InvisibleSpace]", 1],"\"g\"","grams",
           "\"Grams\""},
          "Quantity"]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"doesFloatFlat", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"rightingArm", "[", 
          RowBox[{"boat", ",", "thefront", ",", 
           RowBox[{
            RowBox[{"N", "@", "10"}], " ", "Degree"}]}], "]"}], ">", "0"}], "&&", 
        RowBox[{
         RowBox[{"rightingArm", "[", 
          RowBox[{"boat", ",", "thefront", ",", 
           RowBox[{
            RowBox[{"N", "@", 
             RowBox[{"-", "10"}]}], " ", "Degree"}]}], "]"}], "<", "0"}]}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"trimAngle", "=", 
       RowBox[{
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"avs", "[", 
           RowBox[{"boat", ",", "theside", ",", "0"}], "]"}], "*", 
          RowBox[{"180", "/", "Pi"}]}], "]"}], 
        TemplateBox[{
         InterpretationBox["\[InvisibleSpace]", 1],
          RowBox[{"\[InvisibleSpace]", "\"\[Degree]\""}],"degrees",
          "\"AngularDegrees\""},
         "QuantityPostfix"]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"doesFloatFlat", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"rightingArm", "[", 
          RowBox[{"boat", ",", "thefront", ",", 
           RowBox[{
            RowBox[{"N", "@", "10"}], " ", "Degree"}]}], "]"}], ">", "0"}], "&&", 
        RowBox[{
         RowBox[{"rightingArm", "[", 
          RowBox[{"boat", ",", "thefront", ",", 
           RowBox[{
            RowBox[{"N", "@", 
             RowBox[{"-", "10"}]}], " ", "Degree"}]}], "]"}], "<", "0"}], " ",
         "&&", "\[IndentingNewLine]", 
        RowBox[{
         TemplateBox[{
          RowBox[{"-", "5"}],RowBox[{"\[InvisibleSpace]", "\"\[Degree]\""}],
           "degrees","\"AngularDegrees\""},
          "QuantityPostfix"], "<", "trimAngle", "<", 
         TemplateBox[{
          "5",RowBox[{"\[InvisibleSpace]", "\"\[Degree]\""}],"degrees",
           "\"AngularDegrees\""},
          "QuantityPostfix"]}]}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"mass", "=", 
       RowBox[{
        RowBox[{"totalMass", "[", "boat", "]"}], 
        TemplateBox[{
         InterpretationBox["\[InvisibleSpace]", 1],"\"g\"","grams",
          "\"Grams\""},
         "Quantity"]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"stabilitydata", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\[Theta]", ",", 
           RowBox[{"rightingArm", "[", 
            RowBox[{"boat", ",", "thefront", ",", "\[Theta]"}], "]"}]}], 
          "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[Theta]", ",", 
           RowBox[{"-", "Pi"}], ",", "Pi", ",", 
           RowBox[{"Pi", "/", "30"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"interp", " ", "=", " ", 
       RowBox[{"Interpolation", "[", "stabilitydata", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"stabilityAngles", "=", 
       RowBox[{
        RowBox[{"Sort", "@", 
         RowBox[{"DeleteDuplicates", "@", 
          RowBox[{"Round", "[", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"\[Theta]", "/.", 
               RowBox[{"FindRoot", "[", 
                RowBox[{
                 RowBox[{"interp", "[", "\[Theta]", "]"}], ",", 
                 RowBox[{"{", 
                  RowBox[{"\[Theta]", ",", "i"}], "}"}]}], "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"-", "Pi"}], ",", "Pi", ",", 
                RowBox[{"Pi", "/", "40"}]}], "}"}]}], "]"}], "*", 
            RowBox[{"180", "/", "Pi"}]}], "]"}]}]}], " ", 
