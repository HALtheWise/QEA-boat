           RowBox[{
            RowBox[{"N", "@", "10"}], " ", "Degree"}]}], "]"}], ">", "0"}], "&&", 
        RowBox[{
         RowBox[{"rightingArm", "[", 
          RowBox[{"boat", ",", "thefront", ",", 
           RowBox[{
            RowBox[{"N", "@", 
             RowBox[{"-", "10"}]}], " ", "Degree"}]}], "]"}], "<", "0"}], " ",
         "&&", "\[IndentingNewLine]", 
        RowBox[{
         TemplateBox[{
          RowBox[{"-", "5"}],RowBox[{"\[InvisibleSpace]", "\"\[Degree]\""}],
           "degrees","\"AngularDegrees\""},
          "QuantityPostfix"], "<", "trimAngle", "<", 
